<%= stylesheet_link_tag "edit" %>
<div class="container3">
  <h1 class="editprofile">edit profile</h1>
  <div class="row">
    <form class="col s12">
      <div class="row">
        <form action="#">
          <div class="file-field input-field">
            <div class="btn">
              <span>File</span>
              <input type="file" multiple>
            </div>
            <div class="file-path-wrapper">
              <input class="file-path validate" type="text" placeholder="profile photo">
            </div>
          </div>
        </form>
  <form action="#">
    <input class="datepicker btn" placeholder="Clean/Sobriety Date">
  </form>
        <div class="input-field col s6">
          <input id="first_name" type="text">
          <label for="first_name">First</label>
        </div>
        <div class="input-field col s6">
          <input id="last_name" type="text">
          <label for="last_name">Last</label>
        </div>
      </div>
    </form>
  </div>

  <div class="cleanopt">
    <form action="#">
      <p>
        <input name="group1" type="radio" id="test1" />
        <label for="test1">Clean</label>
       
        <input name="group1" type="radio" id="test2" />
        <label for="test2">Sober</label>
      </p>
    </form>
  </div>

  <div class="input-field col s6">
          <input id="first_name" type="text">
          <label for="home_group">Home Group</label>
  </div>
  <div class="input-field col s6">
    <%= link_to "Done", profile_path, class: "waves-effect waves-light btn" %> 
  </div>
</div>

<script>
  $('.datepicker').pickadate({
    selectMonths: true, // Creates a dropdown to control month
    selectYears: 64, // Creates a dropdown of 15 years to control year,
    min: new Date(1953,9,14),
    max: true,
    closeOnSelect: true,
    closeOnClear: true,
  });
</script>




